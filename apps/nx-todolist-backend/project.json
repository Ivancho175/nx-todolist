{
  "name": "nx-todolist-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nx-todolist-backend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/nx-todolist-backend",
        "main": "apps/nx-todolist-backend/src/main.ts",
        "tsConfig": "apps/nx-todolist-backend/tsconfig.app.json",
        "tsPlugins": [
          {
            "name": "@nestjs/swagger/plugin",
            "options": {
              "dtoFileNameSuffix": [".entity.ts", ".dto.ts"],
              "controllerFileNameSuffix": [".controller.ts"],
              "classValidatorShim": true,
              "dtoKeyOfComment": "description",
              "controllerKeyOfComment": "description",
              "introspectComments": true
            }
          }
        ],
        "assets": ["apps/nx-todolist-backend/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/nx-todolist-backend/webpack.config.js"
      },
      "configurations": {
        "development": {
          "outputPath": "dist/apps/nx-todolist-backend"
        },
        "production": {
          "outputPath": "dist/apps/nx-todolist-backend"
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "nx-todolist-backend:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "nx-todolist-backend:serve:development"
        },
        "production": {
          "buildTarget": "nx-todolist-backend:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/nx-todolist-backend/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/nx-todolist-backend/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
